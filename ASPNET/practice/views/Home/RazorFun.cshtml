
  <h1>Here are some delicious foods!</h1>
  @{
    List<string> FoodList = new List<string>
    { "Apple Pie", "Burritos", "Clams Casino", "Donuts" };
    foreach(string word in FoodList)
    {
      if(word.ToLower().StartsWith("c")) 
      {
        <p style="color: red">@word</p>
      }
      else 
      {
        <p>@word</p>
      }
    }
  }
